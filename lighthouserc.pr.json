{
    "ci": {
        "collect": {
            "staticDistDir": "./public",
            "numberOfRuns": 1,
            "settings": {
                "chromeFlags": "--no-sandbox --disable-gpu --headless --disable-dev-shm-usage"
            }
        },
        "upload": {
            "target": "temporary-public-storage"
        },
        "settings": {
            "emulatedFormFactor": "desktop"
        }
    }
}
